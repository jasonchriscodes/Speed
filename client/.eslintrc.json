{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "prettier", 
        "plugin:react/jsx-runtime", 
        "plugin:jsx-a11y/recommended", 
        "plugin:react-hooks/recommended"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react",
        "jsx-a11y",
        "prettier"
    ],
    "rules": {
        "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
        "semi": ["error", "always"]
    }
}
